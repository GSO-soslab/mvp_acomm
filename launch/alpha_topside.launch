<?xml version="1.0"?>

<launch>
  <arg name="goby_config" default="$(find alpha_comms)/config/goby.yaml"/>
  <arg name="driver_config" default="$(find alpha_comms)/config/evologics.yaml"/>
  <arg name="robot_name" value="camera_frame"/>

  <node ns="$(arg robot_name)" pkg="alpha_comms" type="modem_node" name="usbl_node" output="screen">
    <rosparam command="load" file="$(arg goby_config)"/>
    <rosparam command="load" file="$(arg driver_config)"/>
    
    <param name="type" value="usbl"/>

  </node>

  <node ns="$(arg robot_name)" pkg="alpha_comms" type="alpha_acomms_node" name="alpha_acomms_topside_node" output="screen">
    <rosparam command="load" file="$(arg goby_config)"/> 
    <param name="type" value="usbl"/>

  </node>

  <!-- <node ns="$(arg robot_name)" pkg="nmea_navsat_driver" type="nmea_serial_driver" name="gps_nema_node" output="screen">
    <remap from="fix" to="gps/fix"/>
    <remap from="heading" to="gps/heading"/>
    <remap from="time_reference" to="gps/time_reference"/>
    <remap from="vel" to="gps/vel"/>

    <param name="frame_id" value="$(arg robot_name)/gps"/>
    <param name="port" value="/dev/ttyACM0"/>
    <param name="baud" value="9600"/>
  </node> -->


  

</launch>